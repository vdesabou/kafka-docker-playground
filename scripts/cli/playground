# :command.usage
playground_connector_plugin_search_jar_usage() {
  printf "playground connector-plugin search-jar - ‚òï List jars for a connector plugin from confluent hub https://www.confluent.io/hub/ Search for specific class and display method signatures\n\n"

  printf "%s\n" "$(bold "== Usage ==")"
  printf "  playground connector-plugin search-jar [OPTIONS]\n"
  printf "  playground connector-plugin search-jar --help | -h\n"
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "$(bold "== Options ==")"

    # :command.usage_flags
    # :flag.usage
    printf "  %s\n" "$(magenta "--connector-plugin, -c CONNECTOR-PLUGIN (required)")"
    printf "    üîå Connector plugin name\n    \n    üéì Tip: use <tab> completion to trigger fzf completion\n"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--connector-tag CONNECTOR_TAG")"
    printf "    üîó Connector version to use\n    \n    By default, for each connector, the latest available version on Confluent\n    Hub is used\n    \n    üéì Tip: set to \" \" in order to select the version dynamically\n"
    printf "    %s\n" "Conflicts: --connector-zip"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--class CLASS")"
    printf "    ‚òï Java class name to search for in all jars\n"
    echo

    # :command.usage_fixed_flags
    printf "  %s\n" "$(magenta "--help, -h")"
    printf "    Show this help\n"
    echo

    # :command.usage_examples
    printf "%s\n" "$(bold "Examples")"
    printf "  playground connector-plugin search-jar --connector-plugin\n  confluentinc/kafka-connect-s3 --class WebIdentityTokenCredentialsProvider\n"
    echo

  fi
}

