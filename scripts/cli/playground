    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        playground_connector_offsets_reset_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="connector offsets reset"

  # :command.parse_requirements_while
