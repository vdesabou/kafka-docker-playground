  echo
  # :command.usage_commands
  printf "%s\n" "$(bold "== Commands ==")"
  printf "  %s   üëâ Sync local reproduction-models folder to ec2 instance\n" "$(green "local-to-ec2")"
  printf "  %s   üëà Sync ec2 instance reproduction-models folder to local\n" "$(green "ec2-to-local")"
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "$(bold "== Options ==")"

    # :command.usage_fixed_flags
    printf "  %s\n" "$(magenta "--help, -h")"
    printf "    Show this help\n"
    echo

  fi
}

