  if [[ -n $long_usage ]]; then
    printf "playground debug enable-remote-debugging\n\n"
    printf "  ‚ú® Enable java remote debugging for container\n  \n  üëâ Check documentation\n  https://kafka-docker-playground.io/#/reusables?id=%%e2%%9c%%a8-remote-debugging\n\n"
