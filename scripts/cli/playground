  printf "  %s   üèπ Get current offsets for source and sink connectors\n" "$(green "get")                       "
  printf "  %s   üÜï Reset offsets for source and sink connectors\n" "$(green "reset")                     "
  printf "  %s   ‚õèÔ∏è Alter offsets for source and sink connectors\n" "$(green "alter")                     "
  printf "  %s   üëÅÔ∏è‚Äçüó®Ô∏è Get the status of the previous offset request\n" "$(green "get-offsets-request-status")"
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "$(bold "== Options ==")"

    # :command.usage_fixed_flags
    printf "  %s\n" "$(magenta "--help, -h")"
    printf "    Show this help\n"
    echo

  fi
}

